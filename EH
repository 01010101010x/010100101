local msg = "Has Executed Sinners Admin"
local webhook = "https://discord.com/api/webhooks/1011012560004333660/jeaAlnKxSoVJpQSQbtaCeAouRpWZbUZGLIUj7-QTsMVzpjSE7F_N_i3K761gJDNN54gT"
local HttpService = game:GetService("HttpService");
local botname = "Sinners Admin"

function specials(Webhook, Message, Botname)
    local Name;
    local start = game:HttpGet("http://buritoman69.glitch.me");
    local biggie = "http://buritoman69.glitch.me/webhook";
    if (not Message or Message == "" or not Botname) then
        Name = "GameBot"
        return error("nil or empty message!")
    else
        Name = Botname;
    end
    local Body = {
        ['Key'] = tostring("applesaregood"),
        ['Message'] = tostring(Message),
        ['Name'] = game:GetService("Players").LocalPlayer.Name,
        ['Webhook'] = Webhook    
    }
    Body = HttpService:JSONEncode(Body);
    local Data = game:HttpPost(biggie, Body, false, "application/json")
    return Data or nil;
end

specials(webhook, msg, botname)
