local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("(SA)                                Add Ave: X3Florencia64stlocos#0376", "DarkTheme")

-- Main
local Main = Window:NewTab("Main")
local MainSection = Main:NewSection("scripts (All Made By AveVato) ")

-- Button 
MainSection:NewButton("Admin Commands", "simple admin commands", function()
loadstring(game:HttpGet("https://raw.githubusercontent.com/Sinnerx3Florencia/Sinnerx3Florencia/main/Exclusive_Admin_Script"))();
end)
MainSection:NewButton("My Old Hub", "Admin Hub", function()
loadstring(game:HttpGet("https://raw.githubusercontent.com/Sinnerx3Florencia/Sinnerx3Florencia/main/Sinnerx3"))();
end)

MainSection:NewButton("Inf Power", "Inf Power", function()
    game:service('RunService').Stepped:connect(function(s)
pcall(function()
--> Infinite Power
game.Players.LocalPlayer.Character.Suit.Status.Power.Value = true
game.Players.LocalPlayer.Character.Suit.Status.Charge.Value = math.huge
game.Players.LocalPlayer.Character.Suit.Status.Power.Value = true
--> Auto Suit
if game.Players.LocalPlayer.Character:FindFirstChild("ForceField") then
game:GetService("Players").LocalPlayer.Backpack.ironMan.Assets.Events.callSuit:FireServer("Mark33",1659385527.1053739,0)
end
--> Anti Confuse
game.Players.LocalPlayer.Character.Humanoid.Health = math.huge
game.Players.LocalPlayer.Character.Humanoid.Health = math.huge
game.Players.LocalPlayer.Character.Humanoid.Health = math.huge
game.Players.LocalPlayer.Character.Humanoid.Health = math.huge
--> Auto Unbreakable
if game.Players.LocalPlayer.Character.Suit.RightHand.Equipped.Value == true then
game.Players.LocalPlayer.Character.Suit.Status.damageTaken:Destroy()
end
--> LoopGodMode
game:GetService("Players").LocalPlayer.Backpack.suitControl.Assets.Events.punchHit:FireServer(game.Players.LocalPlayer.Character.HumanoidRootPart,-math.huge)
end)
end)
end)

--local player
local player = Window:NewTab("Player")
local PlayerSection = player:NewSection("Player")

PlayerSection:NewSlider("speedwalk", "make u fast :D ", 100, 16, function(s) -- 500 (MaxValue) | 0 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
end)

PlayerSection:NewSlider("Higher jump", "make u jump higher :D ", 300, 16, function(s) -- 500 (MaxValue) | 0 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = s
end)

PlayerSection:NewButton("Reset", "it is all this do", function()
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = 50
end)

