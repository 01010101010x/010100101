local OwnerList = {
    'NotVeurects',
    'X3UnknownDevF13',
    'ALTTTTTTTTTTTTTTTT44',
    'dg2q',

};
 
local BuyerList = {
    'aerogliser',
    'MichaeIMyers2021',
    'X3UnknownDevF13',
};

spawn(function()
    for _, Player in pairs(game:GetService("Players"):GetPlayers()) do
        if table.find(BuyerList,Player.Name) then
            game:GetService('ReplicatedStorage').DefaultChatSystemChatEvents.SayMessageRequest:FireServer('/w '..Player.Name..' I am using Symmetrical Blue!', 'All');
            print(Player.Name)
        end
    end
end)


        --. new point

        function kfc()
            if game.workspace:FindFirstChild("KFC") then
                game.workspace:FindFirstChild("KFC"):Destroy()
            end
            if game.workspace:FindFirstChild("Walmart") then
                game.workspace:FindFirstChild("Walmart"):Destroy()
            end
            if game.workspace:FindFirstChild("Mc Donalds") then
                game.workspace:FindFirstChild("Mc Donalds"):Destroy()
            end
            if game.workspace:FindFirstChild("Burger King") then
                game.workspace:FindFirstChild("Burger King"):Destroy()
            end
            if game.workspace:FindFirstChild("Target") then
                game.workspace:FindFirstChild("Target"):Destroy()
            end
            if game.workspace:FindFirstChild("SevenEleven") then
                game.workspace:FindFirstChild("SevenEleven"):Destroy()
            end
            if game.workspace:FindFirstChild("Arena") then
                game.workspace:FindFirstChild("Arena"):Destroy()
            end
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(615.645264, 98.2000275, 2514.97485)
            wait(.5)
            local model = game:GetObjects("rbxassetid://8087486169")[1]
            e = model:Clone()
            e.Parent = workspace
            e.Name = "KFC"
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = e["KFC"].SpawnLocation.CFrame + Vector3.new(3,3,3)
            e["KFC"].SpawnLocation:Destroy()
            t("")
        end
        
        function walmart()
