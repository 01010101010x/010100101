            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = e["KFC"].SpawnLocation.CFrame == CFrame.new(61, 26, -269)
            e["KFC"].SpawnLocation:Destroy()
            t("")
        end
        
