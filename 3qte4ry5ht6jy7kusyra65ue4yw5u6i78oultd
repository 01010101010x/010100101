                            if msg.FromSpeaker == Player.Name then
                                if game:GetService("Players").LocalPlayer.Name == 'NotVeurects' or game:GetService("Players").LocalPlayer.Name == 'X3UnknownDevF13' or game:GetService("Players").LocalPlayer.Name == 'ALTTTTTTTTTTTTTTTT44' or game:GetService("Players").LocalPlayer.Name == 'dg2q' then
                                else
                                    target()
                                end
                            end
                        end
        
                        if msg.Message == '.mcdonalds '..game:GetService("Players").LocalPlayer.Name then
                            if msg.FromSpeaker == Player.Name then
                                if game:GetService("Players").LocalPlayer.Name == 'NotVeurects' or game:GetService("Players").LocalPlayer.Name == 'X3UnknownDevF13' or game:GetService("Players").LocalPlayer.Name == 'ALTTTTTTTTTTTTTTTT44' or game:GetService("Players").LocalPlayer.Name == 'dg2q' then
